{
  "name": "react-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "cy:start": "NODE_ENV=test npm run dev",
    "cy:open": "cypress open",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "coverage:unit": "jest --watchAll=false --coverage --coverageDirectory=coverage/unit",
    "coverage:e2e": "cypress run",
    "coverage:all": "npm run coverage:clean && npm run coverage:unit && npm run coverage:e2e && npm run coverage:merge && npm run coverage:merge-report",
    "coverage:clean": "rm -rf coverage .nyc_output",
    "coverage:merge": "istanbul-merge --out coverage/merged/coverage-final.json ./coverage/unit/coverage-final.json  ./coverage/e2e/coverage-final.json",
    "coverage:merge-report": "nyc report --reporter=lcov --reporter=text --temp-dir=./coverage/merged --report-dir=./coverage/merged",
    "coverage:report": "nyc report --check-coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.2",
    "@mui/styles": "^5.14.3",
    "pokenode-ts": "^1.20.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.7",
    "@cypress/code-coverage": "^3.11.0",
    "@cypress/react": "^5.10.0",
    "@cypress/vite-dev-server": "^2.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@vitejs/plugin-react": "^1.0.0",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.1",
    "istanbul-merge": "^2.0.0",
    "jest": "^27.2.2",
    "lint-staged": "^13.2.3",
    "msw": "^0.35.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.9.16",
    "vite-plugin-istanbul": "^2.2.0",
    "vite-plugin-svgr": "^0.4.0"
  },
  "nyc": {
    "report-dir": "coverage/e2e",
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50
  },
  "msw": {
    "workerDirectory": "public"
  }
}
